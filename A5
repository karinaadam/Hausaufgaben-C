#include <stdio.h>

int main(void)
{
    int num, limit=3, count, sum= 0;
    float avg = 0.0;
    
    printf("Enter %d numbers\n", limit);
    for(count = 1; count <= limit; count++)
   {
   scanf ("%d", &num);
   sum=sum + num;
   }
   avg =(float)sum / limit;
   
   printf("Average = %0.2f\n", avg);
   

    return 0;
}
